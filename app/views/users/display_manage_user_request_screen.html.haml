%h2
  Enter User
=form_tag '/manage_user_result', method: 'get' do
  %label{for: :user_string_id}
    %h3
      User Name
  =text_field_tag :user_string, nil, id: :user_string_id, class: 'form-control input-sm'
  %br
  =submit_tag 'Create', class: 'btn btn-primary'
  =submit_tag 'Retire', class: 'btn btn-primary'
  =submit_tag 'Clear Password', class: 'btn btn-primary'
  =submit_tag 'Make Admin', class: 'btn btn-primary'
  =submit_tag 'Remove Admin', class: 'btn btn-primary'
  =submit_tag 'View Details', class: 'btn btn-primary'
